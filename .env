# General Settings
SURL_DATA_HANDLER="postgreSQL"      # Options: "PostgreSQL", default: "local"
SURL_URL_LENGTH=8                   # Short URL ID length, min. 4
# PostgreSQL Server Settings
# (!Required if data handler is "postgreSQL")
SURL_POSTGRESQL_USER="username"     # postgreSQL server username
SURL_POSTGRESQL_PASSWORD="password" # postgreSQL server password
SURL_POSTGRESQL_HOST="host"         # postgeSQL server host address
SURL_POSTGRESQL_PORT=5432           # postgeSQL server port
